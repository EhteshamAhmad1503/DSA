package Shorting;

public class BubbleShort {

	public static void main(String[] args) {

		int nums[] = { 6, 2, 8, 4, 10 };

		for (int i = 1; i < nums.length; i++) // for round 1 to n-1;
		{
			for (int j = 0; j < nums.length; j++)
			{
				// process element til n-i th index

				if (nums[j] > nums[j + 1])
				{
					int temp = nums[j];
					nums[j] = nums[j + 1];
					nums[j + 1] = temp;
				}
				System.out.println(nums[j]+ " ");	
			}
			
				
		}

	}

}
