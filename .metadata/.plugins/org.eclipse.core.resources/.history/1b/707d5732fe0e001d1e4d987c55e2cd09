package Arrays;

import java.util.Scanner;

public class evenSwap {
	public static void main(String args[]) {

		int nums[] = { 3, 5, 7, 11, 6 };

		for (int i = 0; i < nums.length; i++) {

			for (int j = 0; j <= i; j++) {

				if ((nums[i] + nums[j]) % 2 != 0) {
					int temp = nums[i];
					nums[i] = nums[j];
					nums[j] = temp;
				}
			}
		}
		for (int i = 0; i < nums.length; i++) {
			System.out.print(nums[i]);
		}
	}

}
