package Shorting;

public class BubbleShort {

	public static void main(String[] args) {

		int nums[] = {2,8,3,9,5};

		for (int i = 0; i < nums.length-i; i++) // for round 1 to n-1;
		{

			for (int j = 0; j <nums.length -1-i; j++) { // process element til n-i th index

				if (nums[j] > nums[j + 1]) {
					int temp = nums[j];
					nums[j] = nums[j + 1];
					nums[j + 1] = temp;
				}
				
			}
			

		}
		for(int i=0;i<nums.length;i++) {
		System.out.println(nums[i] + " ");
		}
		

	}

}
