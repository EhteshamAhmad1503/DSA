package Shorting;

public class SelectionShort {

	public static void main(String[] args) {

		int arr[] = { 6, 2, 8, 4, 10 };

		for (int i = 0; i < arr.length - 1; i++) {

			if (arr[i] > arr[i + 1]) {
				int temp = arr[i];
				arr[i] = arr[i + 1];
				arr[i + 1] = temp;
			}
			System.out.print(arr[i] + " ");
		}

	}
}
