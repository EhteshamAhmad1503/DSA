package Shorting;

public class BubbleShort {

	public static void main(String[] args) {

		int nums[] = { 2, 8, 3, 9, 10, 1 };

		for (int i = 1; i < nums.length ; i++) // for round 1 to n-1;
		{
			for (int j = 0; j < nums.length - i; j++) { // process element til n-i th index

				if (nums[j] > nums[j + 1]) {
					int temp = nums[j];
					nums[j] = nums[j + 1];
					nums[j + 1] = temp;
				}
			}
		}
		for (int i = 0; i < nums.length; i++) {
			System.out.print(nums[i] + " ");
		}
	}
}
