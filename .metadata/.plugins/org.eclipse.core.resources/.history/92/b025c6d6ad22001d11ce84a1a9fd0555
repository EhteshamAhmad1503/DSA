package Recursion;

import java.util.Scanner;

public class Count {

	static int Print(int n)
	{
		if(n==0)
		{
			return 1;
		}
		System.out.println(n);
		
		int Ans=Print(n-1);
		return Ans;
	}
	public static void main(String[] args) {
	
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter Your Number");
		int n = sc.nextInt();
	}

}
