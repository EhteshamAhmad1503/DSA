package Shorting;

public class BubbleShort {

	public static void main(String[] args) {

		int nums[] = { 6, 2, 8, 4, 10 };

		for (int i = 0; i <= nums.length - i; i++) // for round 1 to n-1;
		{
			int temp = 0;
			for (int j = 0; j <= nums.length - 1; j++) { // process element til n-i th index

				if (nums[j] > nums[j + 1])
				{
					temp = nums[j];
					nums[j] = nums[j + 1];
					nums[j + 1] = temp;
				}
				System.out.println(nums[j] + " ");
			}
			

		}

	}

}
